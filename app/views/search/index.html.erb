<table class="table table-hover">
	<thead>
		<tr>
			<td>Name</td>
			<td>Price</td>
			<td>Buy now</td>

		</tr>
	</thead>
	<%  @data.each do |hash| %>
		<tr>
			<td><a href = "<%=hash["link"]%>"><%=hash["name"]%></a></td>
			<td><%=hash["price"]%></td>
			<td><a href = "<%=hash["link"]%>">Buy at <%=hash["store"]%></a></td>
		<tr>
	<% end %>
</table>